
{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/webhook", "destination": "/api/webhook.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
