
# Exemplo de configuração de ambiente
# Copie este arquivo para .env e preencha com seus valores

# Ambiente
NODE_ENV=development
VITE_NODE_ENV=development

# Configurações de Log
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_PERFORMANCE_LOGS=true

# Supabase (obrigatório)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# API
VITE_API_BASE_URL=http://localhost:8080

# Features
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_ERROR_REPORTING=false

# Configurações de Negócio
VITE_DEFAULT_COMPANY_ID=your_default_company_uuid

# AbacatePay Configuration
VITE_ABACATEPAY_API_URL=https://api.abacatepay.com/v1
VITE_ABACATEPAY_API_KEY_DEV=your_abacatepay_dev_key
VITE_ABACATEPAY_API_KEY_PROD=your_abacatepay_prod_key
